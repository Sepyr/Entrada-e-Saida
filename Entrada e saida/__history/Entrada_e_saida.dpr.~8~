program Entrada_e_saida;

uses
  Vcl.Forms,
  unt_login in 'fontes\unt_login.pas' {frm_login},
  unt_dm in 'fontes\unt_dm.pas' {dtm: TDataModule},
  unt_inicio in 'fontes\unt_inicio.pas' {frm_inicio},
  unt_cadastro_pedagogas in 'fontes\unt_cadastro_pedagogas.pas' {frm_cadastro_pedagogas},
  unt_adm in 'fontes\unt_adm.pas' {frm_adm},
  unt_atrasos in 'fontes\unt_atrasos.pas' {frm_atrasos};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(Tfrm_login, frm_login);
  Application.CreateForm(Tdtm, dtm);
  Application.CreateForm(Tfrm_inicio, frm_inicio);
  Application.CreateForm(Tfrm_cadastro_pedagogas, frm_cadastro_pedagogas);
  Application.CreateForm(Tfrm_adm, frm_adm);
  Application.CreateForm(Tfrm_atrasos, frm_atrasos);
  Application.Run;
end.
